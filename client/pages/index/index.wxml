<view class="index-container">
  <view>
    <map
      id="map"
      longitude="{{longitude}}"
      latitude="{{latitude}}"
      controls="{{controls}}"
      markers="{{markers}}"
      bindmarkertap="markertap"
      bindcontroltap="controltap"
      scale="16"
      show-location
    ></map>
  </view>
  <view>
    <view class="publish" bindtap="publish">发布</view>
    <view class="search" bindtap="search">搜索</view>
  </view>
</view>
